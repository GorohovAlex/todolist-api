{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"$id": "https://example.com/object1590070686.json", 
	"title": "Root", 
	"type": "object",
	"required": [
		"data"
	],
	"properties": {
		"data": {
			"$id": "#root/data", 
			"title": "Data", 
			"type": "array",
			"default": [],
			"items":{
				"$id": "#root/data/items", 
				"title": "Items", 
				"type": "object",
				"required": [
					"type",
					"id",
					"attributes",
					"links"
				],
				"properties": {
					"type": {
						"$id": "#root/data/items/type", 
						"title": "Type", 
						"type": "string",
						"default": "",
						"examples": [
							"tasks"
						],
						"pattern": "^.*$"
					},
					"id": {
						"$id": "#root/data/items/id", 
						"title": "Id", 
						"type": "string",
						"default": "",
						"examples": [
							"1"
						],
						"pattern": "^.*$"
					},
					"attributes": {
						"$id": "#root/data/items/attributes", 
						"title": "Attributes", 
						"type": "object",
						"required": [
							"name",
							"state",
							"deadline",
							"position",
							"created-at",
							"updated-at"
						],
						"properties": {
							"name": {
								"$id": "#root/data/items/attributes/name", 
								"title": "Name", 
								"type": "string",
								"default": "",
								"examples": [
									"smile"
								],
								"pattern": "^.*$"
							},
							"state": {
								"$id": "#root/data/items/attributes/state", 
								"title": "State", 
								"type": "boolean",
								"examples": [
									false
								],
								"default": true
							},
							"deadline": {
								"$id": "#root/data/items/attributes/deadline", 
								"title": "Deadline", 
								"type": "null",
								"default": null
							},
							"position": {
								"$id": "#root/data/items/attributes/position", 
								"title": "Position", 
								"type": "integer",
								"examples": [
									1
								],
								"default": 0
							},
							"created-at": {
								"$id": "#root/data/items/attributes/created-at", 
								"title": "Created-at", 
								"type": "string",
								"default": "",
								"examples": [
									"2020-05-21T14:17:00.883Z"
								],
								"pattern": "^.*$"
							},
							"updated-at": {
								"$id": "#root/data/items/attributes/updated-at", 
								"title": "Updated-at", 
								"type": "string",
								"default": "",
								"examples": [
									"2020-05-21T14:17:00.883Z"
								],
								"pattern": "^.*$"
							}
						}
					}
,
					"links": {
						"$id": "#root/data/items/links", 
						"title": "Links", 
						"type": "object",
						"required": [
							"self"
						],
						"properties": {
							"self": {
								"$id": "#root/data/items/links/self", 
								"title": "Self", 
								"type": "string",
								"default": "",
								"examples": [
									"/api/v1/tasks/1"
								],
								"pattern": "^.*$"
							}
						}
					}

				}
			}

		}
	}
}
